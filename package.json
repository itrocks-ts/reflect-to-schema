{
	"author": {
		"name": "Baptiste Pillot",
		"email": "baptiste@pillot.fr"
	},
	"dependencies": {
		"@itrocks/class-type": "latest",
		"@itrocks/length": "latest",
		"@itrocks/table-schema": "latest",
		"@itrocks/value": "latest"
	},
	"description": "Converts a TypeScript class structure into a table schema",
	"devDependencies": {
		"typescript": "~5.8"
	},
	"engines": {
		"node": ">=18"
	},
	"exports": {
		"import": "./esm/class-to-table.js",
		"require": "./cjs/class-to-table.js"
	},
	"files": [
		"LICENSE",
		"README.md",
		"*/*.d.ts",
		"*/*.js"
	],
	"homepage": "https://it.rocks",
	"keywords": [
		"backend",
		"it.rocks",
		"class",
		"reflect",
		"reflection",
		"schema",
		"structure",
		"table"
	],
	"license": "LGPL-3.0-or-later",
	"name": "@itrocks/class-to-table",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/itrocks-ts/class-to-table.git"
	},
	"scripts": {
		"build": "npm run build:cjs && npm run build:esm",
		"build:cjs": "tsc -p tsconfig.cjs.json",
		"build:esm": "tsc -p tsconfig.esm.json"
	},
	"types": "./esm/class-to-table.d.ts",
	"version": "0.0.1"
}
