{
	"author": {
		"name": "Baptiste Pillot",
		"email": "baptiste@pillot.fr"
	},
	"dependencies": {
		"@itrocks/class-type": "latest",
		"@itrocks/class-view": "latest",
		"@itrocks/former-name": "latest",
		"@itrocks/length": "latest",
		"@itrocks/precision": "latest",
		"@itrocks/property-type": "latest",
		"@itrocks/reflect": "latest",
		"@itrocks/rename": "latest",
		"@itrocks/schema": "latest",
		"@itrocks/store": "latest",
		"@itrocks/value": "latest"
	},
	"description": "Converts a TypeScript class and columns structure into a RDB schema",
	"devDependencies": {
		"typescript": "~5.8"
	},
	"engines": {
		"node": ">=18"
	},
	"exports": {
		"import": "./esm/reflect-to-schema.js",
		"require": "./cjs/reflect-to-schema.js"
	},
	"files": [
		"LICENSE",
		"README.md",
		"*/*.d.ts",
		"*/*.js"
	],
	"homepage": "https://it.rocks",
	"keywords": [
		"backend",
		"it.rocks",
		"class",
		"reflect",
		"reflection",
		"schema",
		"structure",
		"table"
	],
	"license": "LGPL-3.0-or-later",
	"name": "@itrocks/reflect-to-schema",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/itrocks-ts/reflect-to-schema.git"
	},
	"scripts": {
		"build": "npm run build:cjs && npm run build:esm",
		"build:cjs": "tsc -p tsconfig.cjs.json",
		"build:esm": "tsc -p tsconfig.esm.json"
	},
	"types": "./esm/reflect-to-schema.d.ts",
	"version": "0.0.3"
}
